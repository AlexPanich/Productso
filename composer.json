{
    "name": "AlexPanich/productso",
    "type": "library",
    "description": ":package_description",
    "keywords": [
        "AlexPanich",
        "productso"
    ],
    "homepage": "https://github.com/AlexPanich/productso",
    "license": "MIT",
    "authors": [
        {
            "name": "AlexPanich",
            "email": "nasada@yandex.ru",
            "homepage": "http://alexpanich.ru",
            "role": "Developer"
        }
    ],
    "require": {
        "illuminate/support": "~5.1",
        "php" : "~5.5|~7.0"
    },
    "require-dev": {
        "phpunit/phpunit" : "~4.0||~5.0",
        "scrutinizer/ocular": "~1.1",
        "squizlabs/php_codesniffer": "~2.3"
    },
    "autoload": {
        "psr-4": {
            "AlexPanich\\productso\\": "src",
            "AlexPanich\\productso\\Http\\Controllers": "src/Http/Controllers"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "AlexPanich\\productso\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit",
        "format": "phpcbf --standard=psr2 src/"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
